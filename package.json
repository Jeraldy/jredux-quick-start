{
  "name": "jeddy-redux-quick-start",
  "version": "1.0.0",
  "description": "jeddy-redux-quick-start",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack --watch",
    "build": "run-script-os",
    "build:win32": "webpack --config webpack.prod.js && xcopy /s public build",
    "build:darwin:linux": "webpack --config webpack.prod.js && cp -r public build",
    "postinstall": "run-script-os",
    "postinstall:win32": "xcopy /s node_modules\\jeddy-types node_modules\\@types\\jeddy\\",
    "postinstall:darwin:linux": "cp -r node_modules/jeddy-types node_modules/@types/jeddy/"
  },
  "keywords": [],
  "author": "Jeraldy Deus",
  "license": "ISC",
  "devDependencies": {
    "css-loader": "^3.4.2",
    "extract-loader": "^3.1.0",
    "file-loader": "^5.0.2",
    "jeddy-types": "latest",
    "node-sass": "^4.13.1",
    "run-script-os": "^1.1.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^5.1.4",
    "@nx-js/observer-util": "^4.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "jeddy": "latest"
  }
